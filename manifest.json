{
  "manifest_version": 3,
  "name": "MyUW calender converter",
  "version": "1.0",
  "description": "I know the UI sucks",
  "icons": {
    "16": "images/uw_logo_16.png",
    "32": "images/uw_logo_32.png",
    "48": "images/uw_logo_48.png",
    "128": "images/uw_logo_128.png"
  },
  "permissions": ["scripting", "activeTab"],

  "action": {
    "popup": "popup.html",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/uw_logo_16.png",
      "32": "images/uw_logo_32.png",
      "48": "images/uw_logo_48.png",
      "128": "images/uw_logo_128.png"
    }
  }
}